# .env - Environment Configuration
# Place this file in your PROJECT ROOT directory (same level as package.json)

# Backend URL - DO NOT CHANGE THIS
REACT_APP_BACKEND_URL=https://news-impact-screener-backend.onrender.com

# Application Configuration
REACT_APP_VERSION=4.0.0
REACT_APP_ENVIRONMENT=production

# Cache Configuration
REACT_APP_CACHE_DURATION=300000
REACT_APP_CACHE_VERSION=v2.1.0

# API Configuration
REACT_APP_API_TIMEOUT=15000
REACT_APP_MAX_REQUESTS_PER_MINUTE=80
REACT_APP_REQUEST_TIMEOUT=15000

# Performance Monitoring
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ENABLE_DEBUG_LOGS=false

# Feature Flags
REACT_APP_ENABLE_BETA_FEATURES=false
REACT_APP_SERVICE_WORKER_ENABLED=true

# Build Configuration
REACT_APP_BUILD_DATE=