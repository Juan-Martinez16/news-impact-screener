{
  "name": "news-impact-screener",
  "version": "3.2.0",
  "description": "News Impact Screener - Real API Integration",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "backend": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "dev": "concurrently \"npm run backend\" \"npm start\"",
    "verify": "bash verification-script.sh",
    "clean": "rm -rf node_modules backend/node_modules && npm install && cd backend && npm install",
    "deploy:backend": "cd backend && npm run start",
    "deploy:frontend": "npm run build"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
